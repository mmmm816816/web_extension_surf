{
  "name": "surfer",
  "description": "iterate tree structure by onMessage function",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": [
    "https://www.yahoo.co.jp/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}